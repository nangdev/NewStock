{
  "expo": {
    "scheme": "newstock",
    "platforms": ["ios", "android"],
    "name": "NewStock",
    "slug": "NewStock",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/logo.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-secure-store"],
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.b4b4.newstock",
      "googleServicesFile": "./google-services.json",
      "permissions": ["NOTIFICATIONS"],
      "notification": {
        "icon": "./assets/logo.png",
        "color": "#ffffff",
        "channelId": "default"
      }
    },
    "extra": {
      "eas": {
        "projectId": "25923134-7686-4eeb-8e3f-06195c33722f"
      }
    },
    "owner": "ytoko"
  }
}
